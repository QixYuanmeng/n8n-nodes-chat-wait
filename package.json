{
  "name": "n8n-nodes-chat-wait",
  "version": "0.1.1",
  "description": "N8n node for branch flow control and multi-turn conversations with context memory",
  "keywords": [
    "n8n-community-node-package",
    "chat",
    "wait",
    "conversation",
    "memory",
    "webhook",
    "flow-control"
  ],
  "license": "MIT",
  "homepage": "",
